## Ansible configuration for connecting to remote host
# IP address
ansible_host: "{{ vault_hostnames_ip }}"
# User
host_user: "{{ vault_hostnames_user }}"
# User password
host_password: "{{ vault_hostnames_pass }}"
# Admin User
host_admin_user: "{{ vault_hostnames_admin_user }}"
# Admin password
host_admin_password: "{{ vault_hostnames_admin_pass }}"
#Proxy password
host_proxy_pass: "{{ vault_hostnames_proxy_pass }}"
